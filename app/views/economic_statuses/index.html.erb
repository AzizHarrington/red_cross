<h1>Listing economic_statuses</h1>

<table>
  <tr>
    <th>Type name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @economic_statuses.each do |economic_status| %>
  <tr>
    <td><%= economic_status.type_name %></td>
    <td><%= link_to 'Show', economic_status %></td>
    <td><%= link_to 'Edit', edit_economic_status_path(economic_status) %></td>
    <td><%= link_to 'Destroy', economic_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

